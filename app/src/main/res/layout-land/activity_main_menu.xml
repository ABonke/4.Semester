<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/landscape_menu"
    tools:context="com.example.andreas.mandatorycanteenapp.MainMenuActivity">

    <Button
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:text="Take Away"
        android:onClick="TakeAwayNavigate"
        android:layout_toRightOf="@+id/button"
        android:layout_centerVertical="true"
        android:id="@+id/button2"
        android:layout_marginLeft="10dp"/>

    <Button
        android:id="@+id/button"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:onClick="MenuNavigate"
        android:text="Menu"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="45dp" />

    <Button
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:text="Rate Dishes"
        android:onClick="RateNavigate"
        android:layout_toRightOf="@+id/button2"
        android:layout_centerVertical="true"
        android:id="@+id/button3"
        android:layout_marginLeft="10dp"/>

    <Button
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:text="Kalorie tÃ¦ller"
        android:onClick="CalorieNavigate"
        android:layout_toRightOf="@+id/button3"
